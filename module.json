{
  "id": "export-markdown",
  "title": "Markdown Exporter",
  "description": "Exports one or more journals as a ZIP file containing markdown files in a folder tree",
  "version": "#{VERSION}#",
  "library": "false",
  "compatibility": {
    "minimum": 10,
    "verified": 11
  },
  "authors": [
		{
			"name": "Farling",
			"url": "https://github.com/farling42",
			"email": "foundryvtt@amusingtime.uk",
			"discord": "farling",
			"ko-fi": "farling",
			"patreon": "amusingtime",
			"twitter": "@shambles42"
		}
  ],
  "relationships": {
    "requires": [
			{
				"id": "lib-wrapper",
				"manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/latest/download/module.json"
			}
    ]
  },
  "esmodules": [
    "scripts/export-markdown.js"
  ],
  "scripts": [
    "scripts/lib/jszip.min.js",
    "scripts/lib/turndown.js",
    "scripts/lib/turndown-plugin-gfm.js"
  ],
  "styles": [
    "styles/module.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md"
}
